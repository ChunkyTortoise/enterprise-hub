      "prompt_engineering": {"status": "pending", "score": null, "agent": null, "hours": 0},
      "agents": {"status": "pending", "score": null, "agent": null, "hours": 0},
      "langchain": {"status": "pending", "score": null, "agent": null, "hours": 0},
